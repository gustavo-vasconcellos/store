{
  "store": {
    "allowConditions": true,
    "component": "*",
    "around": [
      "storeWrapper",
      "challenge"
    ],
    "before": [
      "header"
    ],
    "after": [
      "footer"
    ],
    "allowed": [
      "notification.bar",
      "carousel",
      "shelf",
      "rebuy",
      "promo-bar",
      "info-card",
      "newsletter",
      "rich-text",
      "flex-layout"
    ],
    "preview": {
      "type": "block"
    }
  },
  "store.home": {
    "around": [
      "homeWrapper"
    ],
    "before": [
      "header.full"
    ],
    "allowed": [
      "search-result"
    ]
  },
  "store.product": {
    "around": [
      "productWrapper"
    ],
    "before": [
      "header.full"
    ],
    "required": [
      "product-details"
    ],
    "allowed": [
      "product-kit",
      "product-reviews",
      "product-questions-and-answers",
      "product-rating-summary",
      "product-rating-inline"
    ],
    "context": "ProductContext"
  },
  "store.search": {
    "around": [
      "searchWrapper"
    ],
    "before": [
      "header.full"
    ],
    "required": [
      "search-result"
    ],
    "context": "SearchContext"
  },
  "store.account": {
    "before": [
      "header.full"
    ],
    "required": [
      "my-account"
    ],
    "around": [
      "challenge.profile"
    ]
  },
  "store.login": {
    "before": [
      "header.full"
    ],
    "required": [
      "login-content"
    ]
  },
  "store.orderplaced": {
    "required": [
      "order-placed"
    ]
  },
  "store.product-review-form": {
    "before": [
      "header.full"
    ],
    "allowed": [
      "product-review-form"
    ]
  },
  "storeWrapper": {
    "allowed": [
      "highlight-overlay"
    ],
    "component": "StoreWrapper"
  },
  "homeWrapper": {
    "component": "HomeWrapper"
  },
  "productWrapper": {
    "component": "ProductWrapper"
  },
  "searchWrapper": {
    "component": "SearchWrapper"
  },
  "challenge": {
    "component": "DefaultChallenge",
    "extensible": "public"
  },
  "highlight-overlay": {
    "component": "*",
    "extensible": "vtex"
  },
  "challenge.profile": {
    "component": "ProfileChallenge"
  },
  "promo-bar": {
    "component": "*"
  },
  "store.custom": {}
}
